@model IEnumerable<Incedo_Octavius_Demo_2.Models.degree_master_table>

@{

    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Degree)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Degree)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.DegreeID }) |
                @Html.ActionLink("Details", "Details", new { id=item.DegreeID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.DegreeID })
            </td>
        </tr>

    }

</table>
